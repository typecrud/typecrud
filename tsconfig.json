{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"declaration": true,
		"outDir": "./lib",
		"strict": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2015"
		]
	},
	"include": [
		"src",
		"models"
	],
	"exclude": [
		"node_modules",
		"**/test/*"
	]
}